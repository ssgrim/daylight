# Frontend Environment Variables - Production
# Copy this file to .env.production and fill in your values

# API Configuration (Replace with your actual API Gateway URL)
VITE_API_BASE=https://your-api-id.execute-api.us-west-1.amazonaws.com

# External Services
VITE_MAPBOX_TOKEN=pk.your_production_mapbox_token_here

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_MOCK_DATA=false

# Error Reporting
VITE_SENTRY_DSN=https://your_production_sentry_dsn@sentry.io/project_id
VITE_ENVIRONMENT=production

# Application Info
VITE_APP_NAME=Daylight
VITE_APP_VERSION=0.2.0
